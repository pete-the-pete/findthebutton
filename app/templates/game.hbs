<div class="row">
  <div class="col-xs-12 col-sm-12 col-md-10">
    <div class="row">
      <div class="col-md-2">
        {{game-info currentLevel=model.currentLevel levelTitle=model.levelTitle isPlaying=model.isPlaying isWinner=model.isWinner}}
      </div>
      <div class="col-md-8">
        <div id="level">
          {{level-info currentLevel=model.currentLevel isPlaying=model.isPlaying isWinner=model.isWinner startPlaying="startPlaying"}}
          {{component (concat 'levels/' model.levelFile) currentLevel=model.currentLevel isPlaying=model.isPlaying advanceLevel="advanceLevel" }}
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-2 hidden-md text-centered">
    I AM AN ADVERTISEMENT
  </div>
</div>